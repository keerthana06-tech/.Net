<h2>Login Page</h2>

<form method="post">
    <div>
        <label for="username">,Username:></label>
        <input type="text"  name="username" required />
    </div>
    <div>
        <label for="password">Password:</label>
        <input type="password"  name="password" required />
    </div>
    <div>
        <button type="submit">Login</button>
        
    </div>
</form>
@ViewBag.Error
